; --proof-with-sharing --index-fresh-sorts --proof-define-skolems --proof-prune --proof-merge --disable-print-success --disable-banner --max-time=30
(set-option :produce-proofs true)
(set-logic AUFLIRA)
(declare-sort Nat$ 0)
(declare-fun c$ () Real)
(declare-fun f$ (Nat$) Real)
(declare-fun g$ (Nat$) Real)
(declare-fun n$ () Nat$)
(declare-fun r$ () Real)
(declare-fun no$ () Nat$)
(declare-fun times$ (Real Real) Real)
(declare-fun less_eq$ (Nat$ Nat$) Bool)
(assert (! (not (less_eq$ no$ n$)) :named a0))
(assert (! (< 0.0 c$) :named a1))
(assert (! (< 0.0 r$) :named a2))
(assert (! (<= (ite (< (f$ n$) 0.0) (- (f$ n$)) (f$ n$)) (times$ c$ (ite (< (g$ n$) 0.0) (- (g$ n$)) (g$ n$)))) :named a3))
(assert (! (less_eq$ no$ n$) :named a4))
(check-sat)
