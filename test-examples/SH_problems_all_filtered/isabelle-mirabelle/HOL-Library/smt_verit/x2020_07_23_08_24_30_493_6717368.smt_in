; --proof-with-sharing --index-fresh-sorts --proof-define-skolems --proof-prune --proof-merge --disable-print-success --disable-banner --max-time=20
(set-option :produce-proofs true)
(set-logic AUFLIA)
(declare-sort B$ 0)
(declare-fun abs$ (B$) B$)
(declare-fun sgn$ (B$) B$)
(declare-fun less$ (B$ B$) Bool)
(declare-fun zero$ () B$)
(declare-fun times$ (B$ B$) B$)
(declare-fun uminus$ (B$) B$)
(assert (! (forall ((?v0 B$)) (= (abs$ ?v0) (ite (less$ ?v0 zero$) (uminus$ ?v0) ?v0))) :named a0))
(assert (! (forall ((?v0 B$)) (= (abs$ ?v0) (times$ ?v0 (sgn$ ?v0)))) :named a1))
(assert (! (not (forall ((?v0 B$)) (= (abs$ ?v0) (times$ ?v0 (sgn$ ?v0))))) :named a2))
(check-sat)
