; --full-saturate-quant --inst-when=full-last-call --inst-no-entail --term-db-mode=relevant --multi-trigger-linear --no-statistics --random-seed=1 --lang=smt2 --continued-execution --tlimit 30000
(set-option :produce-unsat-cores true)
(set-logic AUFLIRA)
(declare-sort Nat$ 0)
(declare-fun i$ () Nat$)
(declare-fun k$ () Nat$)
(declare-fun l$ () Nat$)
(declare-fun p$ () Nat$)
(declare-fun x$ () Real)
(declare-fun plus$ (Nat$ Nat$) Nat$)
(declare-fun power$ (Nat$ Nat$) Nat$)
(declare-fun times$ (Nat$ Nat$) Nat$)
(declare-fun of_nat$ (Nat$) Real)
(assert (! (not (<= (of_nat$ (power$ p$ (plus$ (times$ k$ i$) l$))) x$)) :named a0))
(assert (! (<= (of_nat$ (power$ p$ (plus$ (times$ k$ i$) l$))) x$) :named a1))
(check-sat)
(get-unsat-core)
